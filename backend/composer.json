{
    "name": "kinematic/dam",
    "description": "A digital asset management platform",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Leo Baker-Hytch",
            "email": "ltobh@tuta.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "laravel/framework": "^12.0",
        "laravel/tinker": "^2.10.1",
        "nuwave/lighthouse": "^6.63",
        "mll-lab/laravel-graphiql": "^4.0",
        "tymon/jwt-auth": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.3",
        "laravel/telescope": "^5.12"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan lighthouse:ide-helper"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": [
                "laravel/telescope"
            ]
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
